# Level114 Subnet Configuration Example
# Copy this file to .env and customize the values

# Network Configuration
LEVEL114_NETUID=114
LEVEL114_NETWORK=finney

# Wallet Configuration
LEVEL114_WALLET_NAME=default
LEVEL114_WALLET_HOTKEY=default
LEVEL114_WALLET_PATH=~/.bittensor/wallets

# Collector Configuration
LEVEL114_COLLECTOR_URL=http://collector.level114.io:3000

# Logging Configuration
LEVEL114_LOG_LEVEL=INFO
LEVEL114_LOG_FILE=

# Performance Configuration
LEVEL114_REQUEST_TIMEOUT=30.0
LEVEL114_MAX_RETRIES=3
LEVEL114_RETRY_DELAY=1.0

# Metrics Configuration
LEVEL114_METRICS_INTERVAL=60.0
LEVEL114_METRICS_HISTORY_SIZE=1000

# Development Configuration
LEVEL114_DEBUG=false
LEVEL114_DEV_MODE=false

# Miner Specific Configuration
LEVEL114_MINER_MINER_PORT=8091
LEVEL114_MINER_MINER_IP=
LEVEL114_MINER_AUTO_REGISTER=true
LEVEL114_MINER_REGISTRATION_INTERVAL=3600.0
LEVEL114_MINER_MAX_CONCURRENT_TASKS=10
LEVEL114_MINER_TASK_TIMEOUT=300.0
LEVEL114_MINER_HEALTH_CHECK_INTERVAL=30.0

# Validator Specific Configuration
LEVEL114_VALIDATOR_VALIDATION_INTERVAL=60.0
LEVEL114_VALIDATOR_WEIGHT_SETTING_INTERVAL=300.0
LEVEL114_VALIDATOR_QUERY_TIMEOUT=10.0
LEVEL114_VALIDATOR_MIN_MINERS_TO_QUERY=5
LEVEL114_VALIDATOR_MAX_MINERS_PER_QUERY=50
LEVEL114_VALIDATOR_MIN_WEIGHT_THRESHOLD=0.001
LEVEL114_VALIDATOR_MAX_WEIGHT_PER_MINER=1.0
